# AWS Configuration
aws_region = "us-east-1"

# Domain Configuration
domain_name = "your-n8n-domain.com"
admin_email = "admin@your-n8n-domain.com"

# EC2 Configuration
instance_type = "t3.medium"
key_pair_name = "your-key-pair-name"

# Database Configuration
db_instance_class = "db.t3.micro"
db_password      = "your-secure-database-password"

# Network Configuration
ssh_cidr = "0.0.0.0/0"  # Change this to your IP for security

# Route53 Configuration (optional)
route53_zone_id = "Z1234567890ABC"  # Your Route53 hosted zone ID 